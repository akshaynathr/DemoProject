                      partialTriggers="::t1">
              <af:column headerText="Role" id="c14">
                <af:selectOneChoice value="#{row.bindings.RoleId.inputValue}" label="#{row.bindings.RoleId.label}"
                                    required="#{bindings.UsersRolesU1.hints.RoleId.mandatory}"
                                    shortDesc="#{bindings.UsersRolesU1.hints.RoleId.tooltip}" readOnly="true" id="soc5">
                  <f:selectItems value="#{row.bindings.RoleId.items}" id="si5"/>
                  <f:validator binding="#{row.bindings.RoleId.validator}"/>
                </af:selectOneChoice>
              </af:column>
              <af:column headerText="Start Date" id="c19">
                <af:outputText value="#{row.StartDate}" shortDesc="#{bindings.UsersRolesU1.hints.StartDate.tooltip}"
                               id="ot1">
                  <af:convertDateTime pattern="#{bindings.UsersRolesU1.hints.StartDate.format}"/>
                </af:outputText>
              </af:column>
              <af:column headerText="End Date" id="c20">
                <af:outputText value="#{row.EndDate}" shortDesc="#{bindings.UsersRolesU1.hints.EndDate.tooltip}"
                               id="ot14">
                  <af:convertDateTime pattern="#{bindings.UsersRolesU1.hints.EndDate.format}"/>
                </af:outputText>
              </af:column>
            </af:table>
          </af:panelGroupLayout>
        </af:showDetailItem>
        <af:showDetailItem id="sdi1" icon="#{resource['images:paper-box-icon-4352.png']}"
