<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="test-flow">
    <default-activity>dashboard</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__28">
      <managed-bean-name>InsertBean</managed-bean-name>
      <managed-bean-class>view.insertBean</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__29">
      <managed-bean-name>InsertBean_OP_REVENUE</managed-bean-name>
      <managed-bean-class>view.InsertBeanOPRevenu</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="dashboard">
      <page>/dashboard.jsff</page>
    </view>
    <view id="SalesInside">
      <page>/SalesInside.jsff</page>
    </view>
    <task-flow-call id="customer-flow">
      <task-flow-reference>
        <document>/WEB-INF/customer-flow.xml</document>
        <id>customer-flow</id>
      </task-flow-reference>
      <input-parameter id="__20">
        <name>custId</name>
        <value>#{pageFlowScope.fromcustId}</value>
        <pass-by-value/>
      </input-parameter>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <view id="report_sales_manager">
      <page>/report_sales_manager.jsff</page>
    </view>
    <task-flow-call id="oppurtunity-flow">
      <task-flow-reference>
        <document>/WEB-INF/oppurtunity-flow.xml</document>
        <id>oppurtunity-flow</id>
      </task-flow-reference>
      <input-parameter id="__21">
        <name>optyId</name>
        <value>#{pageFlowScope.fromOptyId}</value>
        <pass-by-value/>
      </input-parameter>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <task-flow-call id="create_customer_flow">
      <task-flow-reference>
        <document>/WEB-INF/create_customer_flow.xml</document>
        <id>create_customer_flow</id>
      </task-flow-reference>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <view id="report_sales_rep">
      <page>/report_sales_rep.jsff</page>
    </view>
    <task-flow-call id="create-opportunity-flow">
      <task-flow-reference>
        <document>/WEB-INF/create-opportunity-flow.xml</document>
        <id>create-opportunity-flow</id>
      </task-flow-reference>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <task-flow-call id="editTasks-flow">
      <task-flow-reference>
        <document>/WEB-INF/editTasks-flow.xml</document>
        <id>editTasks-flow</id>
      </task-flow-reference>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>dashboard</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>inside</from-outcome>
        <to-activity-id>SalesInside</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>report_sales_manager</from-outcome>
        <to-activity-id>report_sales_manager</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__19">
        <from-outcome>report_sales_rep</from-outcome>
        <to-activity-id>report_sales_rep</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>SalesInside</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>back</from-outcome>
        <to-activity-id>dashboard</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>customer</from-outcome>
        <to-activity-id>customer-flow</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__11">
        <from-outcome>oppurtunity_flow</from-outcome>
        <to-activity-id>oppurtunity-flow</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>create_customer</from-outcome>
        <to-activity-id>create_customer_flow</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__22">
        <from-outcome>create_opportunity</from-outcome>
        <to-activity-id>create-opportunity-flow</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__25">
        <from-outcome>edit_tasks</from-outcome>
        <to-activity-id>editTasks-flow</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>customer-flow</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>return_customer</from-outcome>
        <to-activity-id>SalesInside</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>report_sales_manager</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>back</from-outcome>
        <to-activity-id>dashboard</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>oppurtunity-flow</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>return_oppurtunity_flow</from-outcome>
        <to-activity-id>SalesInside</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__17">
      <from-activity-id>create_customer_flow</from-activity-id>
      <control-flow-case id="__18">
        <from-outcome>return_customer_create</from-outcome>
        <to-activity-id>SalesInside</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>report_sales_rep</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>back</from-outcome>
        <to-activity-id>dashboard</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__23">
      <from-activity-id>create-opportunity-flow</from-activity-id>
      <control-flow-case id="__24">
        <from-outcome>create_opportunity_return</from-outcome>
        <to-activity-id>SalesInside</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__26">
      <from-activity-id>editTasks-flow</from-activity-id>
      <control-flow-case id="__27">
        <from-outcome>taskFlowReturn1</from-outcome>
        <to-activity-id>SalesInside</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
