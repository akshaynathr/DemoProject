    <task-flow-call id="UserTrain1">
      <task-flow-reference>
        <document>/WEB-INF/UserTrain.xml</document>
        <id>UserTrain</id>
      </task-flow-reference>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <task-flow-call id="EditUser-flow">
      <task-flow-reference>
        <document>/WEB-INF/EditUser-flow.xml</document>
        <id>EditUser-flow</id>
      </task-flow-reference>
      <run-as-dialog>
        <display-type>
          <inline-popup/>
        </display-type>
      </run-as-dialog>
    </task-flow-call>
    <task-flow-call id="editProduct-flow">
      <task-flow-reference>
        <document>/WEB-INF/editProduct-flow.xml</document>
