<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="oppurtunity-flow">
    <default-activity>oppurtunity_tab</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="oppurtunity_tab">
      <page>/oppurtunity_tab.jsf</page>
    </view>
    <task-flow-return id="return_oppurtunity_flow">
      <outcome>
        <name>return_oppurtunity_flow</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>oppurtunity_tab</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>return</from-outcome>
        <to-activity-id>return_oppurtunity_flow</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
