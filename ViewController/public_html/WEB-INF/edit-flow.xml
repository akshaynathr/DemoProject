<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="edit-flow">
    <default-activity>router1</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="contact_edit"></view>
    <view id="opportunity_edit"></view>
    <view id="customer_edit"></view>
    <view id="appointment_edit"></view>
    <view id="task_edit"></view>
    <router id="router1"></router>
    <task-flow-return id="save">
      <outcome>
        <name>save</name>
      </outcome>
    </task-flow-return>
    <task-flow-return id="cancel">
      <outcome>
        <name>cancel</name>
      </outcome>
    </task-flow-return>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
