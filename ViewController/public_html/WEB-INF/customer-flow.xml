<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="customer-flow">
    <default-activity>Customers_tab</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__8">
      <name>inputParameter1</name>
      <value>#{pageFlowScope.toVal}</value>
      <class>java.lang.String</class>
      <required/>
    </input-parameter-definition>
    <managed-bean id="__3">
      <managed-bean-name>backing_edit_customer</managed-bean-name>
      <managed-bean-class>view.backing.Edit_customer</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
      <!--oracle-jdev-comment:managed-bean-jsp-link:1edit_customer.jsf-->
    </managed-bean>
    <view id="Customers_tab">
      <page>/Customers_tab.jsf</page>
    </view>
    <task-flow-return id="return_customer">
      <outcome>
        <name>return_customer</name>
      </outcome>
    </task-flow-return>
    <view id="edit_customer">
      <display-name>Edit Customer</display-name>
      <page>/edit_customer.jsf</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>Customers_tab</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>return</from-outcome>
        <to-activity-id>return_customer</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <from-outcome>edit_customer</from-outcome>
        <to-activity-id>edit_customer</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>edit_customer</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>return_edit_customer</from-outcome>
        <to-activity-id>Customers_tab</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
