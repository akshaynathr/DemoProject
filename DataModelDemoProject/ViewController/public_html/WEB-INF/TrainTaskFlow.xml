<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="TrainTaskFlow">
    <default-activity>CreateInsert</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__6">
      <managed-bean-name>backing_Personal</managed-bean-name>
      <managed-bean-class>com.oracle.view.backing.Personal</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
      <!--oracle-jdev-comment:managed-bean-jsp-link:1Personal.jsf-->
    </managed-bean>
    <managed-bean id="__7">
      <managed-bean-name>backing_Corporate</managed-bean-name>
      <managed-bean-class>com.oracle.view.backing.Corporate</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
      <!--oracle-jdev-comment:managed-bean-jsp-link:1Corporate.jsf-->
    </managed-bean>
    <managed-bean id="__8">
      <managed-bean-name>backing_Summary</managed-bean-name>
      <managed-bean-class>com.oracle.view.backing.Summary2</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
      <!--oracle-jdev-comment:managed-bean-jsp-link:1Summary.jsf-->
    </managed-bean>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>newUser</fixed-outcome>
      </outcome>
    </method-call>
    <view id="Personal">
      <page>/Personal.jsf</page>
      <train-stop>
        <display-name>Personal Details</display-name>
      </train-stop>
    </view>
    <view id="Corporate">
      <page>/Corporate.jsf</page>
      <train-stop>
        <display-name>Corporate Details</display-name>
      </train-stop>
    </view>
    <view id="Summary">
      <page>/Summary.jsf</page>
      <train-stop>
        <display-name>Summary</display-name>
      </train-stop>
    </view>
    <task-flow-return id="Submit">
      <outcome>
        <name>Submit</name>
        <commit/>
      </outcome>
    </task-flow-return>
    <task-flow-return id="Cancel">
      <outcome>
        <name>Cancel</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <train/>
    <control-flow-rule id="__1">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>newUser</from-outcome>
        <to-activity-id>Personal</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>Summary</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>submit</from-outcome>
        <to-activity-id>Submit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>rollback</from-outcome>
        <to-activity-id>Cancel</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
