<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ForecastLookupVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ForecastLookupVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT DISTINCT                          
    OPPORTUNITIES.QUARTER_NO,                          
    SUM(OPPORTUNITIES.REVENUE) AS Revenue                          
FROM                          
    USERS,                          
    OPPORTUNITIES                          
WHERE                          
    OPPORTUNITIES.IS_ACTIVE != 'n' AND USERS.IS_ACTIVE != 'n' AND IS_INCLUDED='y' AND OPPORTUNITIES.CREATED_BY_ID = USERS.USER_ID AND (USERS.SUPERVISOR_ID = 4 OR USERS.USER_ID=4) AND OPPORTUNITIES.END_DATE < TO_TIMESTAMP('31-MAR-19 05.53.57.120000000 AM') AND  OPPORTUNITIES.START_DATE > TO_TIMESTAMP('01-APR-18 05.53.57.120000000 AM')   
    GROUP BY                        
    OPPORTUNITIES.QUARTER_NO]]></SQLQuery>
  <ViewAttribute
    Name="OppurtunityName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="OPPURTUNITY_NAME"
    Expression="OPPURTUNITY_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Revenue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="2"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="REVENUE"
    Expression="REVENUE"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.lookup.common.ForecastLookupVOMsgBundle"/>
  </ResourceBundle>
</ViewObject>
