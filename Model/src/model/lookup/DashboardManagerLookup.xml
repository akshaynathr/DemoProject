<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DashboardManagerLookup"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  OrderBy='"QUARTER_NO"'>
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="DashboardManagerLookup_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT DISTINCT           
    OPPORTUNITIES.QUARTER_NO,           
    SUM(OPPORTUNITIES.BEST_CASE_REVENUE) AS "Best Case Revenue" ,SUM(OPPORTUNITIES.WORST_CASE_REVENUE) AS "Worst Case Revenue" , SUM(OPPORTUNITIES.REVENUE) AS Revenue     
FROM           
    USERS,           
    OPPORTUNITIES           
WHERE           
    OPPORTUNITIES.IS_ACTIVE != 'n' AND USERS.IS_ACTIVE != 'n' AND OPPORTUNITIES.CREATED_BY_ID = USERS.USER_ID AND (USERS.SUPERVISOR_ID = 4 OR USERS.USER_ID=4) AND OPPORTUNITIES.END_DATE < TO_TIMESTAMP('31-MAR-18 05.53.57.120000000 AM') AND  OPPORTUNITIES.START_DATE > TO_TIMESTAMP('01-APR-17 05.53.57.120000000 AM')          
GROUP BY           
    OPPORTUNITIES.QUARTER_NO]]></SQLQuery>
  <ViewAttribute
    Name="QuarterNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="QUARTER_NO"
    Expression="QUARTER_NO"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="BestCaseRevenue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName='"Best Case Revenue"'
    Expression='"Best Case Revenue"'
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="WorstCaseRevenue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName='"Worst Case Revenue"'
    Expression='"Worst Case Revenue"'
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Revenue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="REVENUE"
    Expression="REVENUE"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.lookup.common.DashboardManagerLookupMsgBundle"/>
  </ResourceBundle>
</ViewObject>
