<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ContactNameLookupjVO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ContactNameLookupjVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT CONTACTS.FIRST_NAME||' '||CONTACTS.LAST_NAME || ' OF  ' ||CUSTOMERS.CUSTOMER_NAME   ,        
    CONTACTS.CONTACT_ID            
FROM           
    CONTACTS,           
    CUSTOMERS_CONTACTS,           
    OPPORTUNITIES,           
    CONTACTS_OPPORTUNITIES      ,CUSTOMERS     
WHERE           
    CONTACTS.CONTACT_ID = CUSTOMERS_CONTACTS.CONTACT_ID AND OPPORTUNITIES.CUSTOMER_ID = CUSTOMERS_CONTACTS.CUSTOMER_ID AND OPPORTUNITIES.OPPORTUNITY_ID = CONTACTS_OPPORTUNITIES.OPPORTUNITY_ID  AND CUSTOMERS.CUSTOMER_ID=CUSTOMERS_CONTACTS.CUSTOMER_ID AND CONTACTS.CONTACT_ID not in (select customers_contacts.contact_id from customers_contacts)]]></SQLQuery>
  <ViewAttribute
    Name="Selectobjects0"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="156"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SELECTOBJECTS0"
    Expression="SELECTOBJECTS0"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="156"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ContactId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="java.lang.Long"
    ColumnType="NUMBER"
    AliasName="CONTACT_ID"
    Expression="CONTACT_ID"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="ContactId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.lookup.common.ContactNameLookupjVOMsgBundle"/>
  </ResourceBundle>
</ViewObject>
